# makefile for include/

INSTALL		= @INSTALL@
INSTALL_DATA	= @INSTALL_DATA@
RM		= @RM@

prefix		= $(DESTDIR)@prefix@
includedir	= $(DESTDIR)@includedir@

all:

install:
	mkdir -p $(includedir)/ircd-hybrid-7
	@echo Installing header files...

	@for header in *.h; do \
		${INSTALL_DATA} $$header $(includedir)/ircd-hybrid-7; \
	done

distclean:
	${RM} -f Makefile

.PHONY: install distclean
